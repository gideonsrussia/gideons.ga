    "metalsmith-templates": "^0.7.0",
    "moment": "^2.11.2",
    "nodemon": "^1.3.7",
    "pug": "^2.0.0-rc.4",
    "serve": "~> 7.0.0",
    "write-good": "^0.9.1"
