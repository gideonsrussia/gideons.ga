    "asciidoctor.js": "^1.5.6-preview.3",
    "coffee-script": "^1.10.0",
    "gulpsmith": "^0.5.4",
    "highlighter": "^0.0.3",
    "jade": "^1.9.2",
    "metalsmith": "^1.7.0",
    "metalsmith-asciidoc": "^1.1.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-autoprefixer": "^1.0.1",
